{
    "name": "csspectrum",
    "version": "0.4.0",
    "description": "A TypeScript library for working with CSS color formats, providing conversion, manipulation, and analysis tools for various color models and spaces.",
    "type": "commonjs",
    "private": false,
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs",
            "types": "./dist/index.d.ts"
        },
        "./package.json": "./package.json",
        "./README.md": "./README.md",
        "./LICENSE": "./LICENSE"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/ganemedelabs/csspectrum.git"
    },
    "author": "ganemedelabs",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ganemedelabs/csspectrum/issues"
    },
    "homepage": "https://github.com/ganemedelabs/csspectrum#readme",
    "keywords": [
        "color",
        "color-converter",
        "css-colors",
        "hex",
        "color-names",
        "rgba",
        "rgb",
        "hsl",
        "hsla",
        "hwb",
        "lab",
        "lch",
        "oklab",
        "oklch",
        "color-spaces",
        "color-mix",
        "relative-colors",
        "srgb",
        "srgb-linear",
        "display-p3",
        "rec2020",
        "a98-rgb",
        "prophoto-rgb",
        "xyz",
        "xyz-d50",
        "xyz-d65"
    ],
    "files": [
        "dist",
        "README.md",
        "LICENSE",
        "package.json"
    ],
    "scripts": {
        "build": "tsc",
        "test": "jest",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "format": "prettier --write \"./**/*.{js,ts,json}\""
    },
    "devDependencies": {
        "@eslint/compat": "^1.2.3",
        "@eslint/eslintrc": "^3.2.0",
        "@eslint/js": "^9.16.0",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.10.1",
        "@typescript-eslint/eslint-plugin": "^8.16.0",
        "@typescript-eslint/parser": "^8.16.0",
        "eslint": "^9.16.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-prettier": "^5.2.1",
        "jest": "^29.7.0",
        "jiti": "^2.4.2",
        "prettier": "^3.4.1",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.2",
        "typescript": "^5.7.2"
    }
}
