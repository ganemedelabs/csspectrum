{
    "name": "saturon",
    "version": "0.1.0",
    "description": "A runtime-extensible JavaScript library for parsing, converting, and manipulating colors with full CSS spec support.",
    "type": "module",
    "private": false,
    "author": "ganemedelabs",
    "license": "MIT",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./config": {
            "import": "./dist/config.js",
            "types": "./dist/config.d.ts"
        },
        "./Color": {
            "import": "./dist/Color.js",
            "types": "./dist/Color.d.ts"
        },
        "./types": {
            "import": "./dist/types.js",
            "types": "./dist/types.d.ts"
        },
        "./utils": {
            "import": "./dist/utils.js",
            "types": "./dist/utils.d.ts"
        },
        "./converters": {
            "import": "./dist/converters.js",
            "types": "./dist/converters.d.ts"
        },
        "./math": {
            "import": "./dist/math.js",
            "types": "./dist/math.d.ts"
        },
        "./plugins/harmony": {
            "import": "./dist/plugins/harmony.js",
            "types": "./dist/plugins/harmony.d.ts"
        },
        "./plugins/accessibility": {
            "import": "./dist/plugins/accessibility.js",
            "types": "./dist/plugins/accessibility.d.ts"
        }
    },
    "typesVersions": {
        "*": {
            "index.d.ts": [
                "dist/index.d.ts"
            ],
            "config.d.ts": [
                "dist/config.d.ts"
            ],
            "Color.d.ts": [
                "dist/Color.d.ts"
            ],
            "types.d.ts": [
                "dist/types.d.ts"
            ],
            "utils.d.ts": [
                "dist/utils.d.ts"
            ],
            "converters.d.ts": [
                "dist/converters.d.ts"
            ],
            "math.d.ts": [
                "dist/math.d.ts"
            ],
            "plugins/harmony.d.ts": [
                "dist/plugins/harmony.d.ts"
            ],
            "plugins/accessibility.d.ts": [
                "dist/plugins/accessibility.d.ts"
            ]
        }
    },
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/ganemedelabs/saturon.git"
    },
    "bugs": {
        "url": "https://github.com/ganemedelabs/saturon/issues"
    },
    "homepage": "https://github.com/ganemedelabs/saturon#readme",
    "scripts": {
        "build": "tsc",
        "test": "jest",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "format": "prettier --write \"./**/*.{js,ts,json}\""
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE",
        "package.json"
    ],
    "keywords": [
        "color-converter",
        "css-colors",
        "w3c",
        "color",
        "color-base",
        "hex-color",
        "named-color",
        "color-function",
        "light-dark",
        "color-mix",
        "transparent",
        "contrast-color",
        "device-cmyk",
        "currentcolor",
        "system-color"
    ],
    "devDependencies": {
        "@eslint/compat": "^1.2.3",
        "@eslint/eslintrc": "^3.2.0",
        "@eslint/js": "^9.16.0",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.10.1",
        "@typescript-eslint/eslint-plugin": "^8.16.0",
        "@typescript-eslint/parser": "^8.16.0",
        "eslint": "^9.16.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-prettier": "^5.2.1",
        "jest": "^29.7.0",
        "jiti": "^2.4.2",
        "prettier": "^3.4.1",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.2",
        "typescript": "^5.7.2"
    }
}
