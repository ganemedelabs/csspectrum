{
    "name": "saturon",
    "version": "0.1.0",
    "description": "A TypeScript library for working with CSS color formats, providing conversion, manipulation, and analysis tools for various color models and spaces.",
    "type": "module",
    "private": false,
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./package.json": "./package.json",
        "./README.md": "./README.md",
        "./LICENSE": "./LICENSE"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/ganemedelabs/saturon.git"
    },
    "author": "ganemedelabs",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ganemedelabs/saturon/issues"
    },
    "homepage": "https://github.com/ganemedelabs/saturon#readme",
    "files": [
        "dist",
        "README.md",
        "LICENSE",
        "package.json"
    ],
    "scripts": {
        "build": "tsc",
        "test": "jest",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "format": "prettier --write \"./**/*.{js,ts,json}\""
    },
    "devDependencies": {
        "@eslint/compat": "^1.2.3",
        "@eslint/eslintrc": "^3.2.0",
        "@eslint/js": "^9.16.0",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.10.1",
        "@typescript-eslint/eslint-plugin": "^8.16.0",
        "@typescript-eslint/parser": "^8.16.0",
        "eslint": "^9.16.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-prettier": "^5.2.1",
        "jest": "^29.7.0",
        "jiti": "^2.4.2",
        "prettier": "^3.4.1",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.2",
        "typescript": "^5.7.2"
    },
    "keywords": [
        "color-converter",
        "css-colors",
        "w3c",
        "color",
        "color-base",
        "hex-color",
        "named-color",
        "color-function",
        "light-dark",
        "color-mix",
        "transparent",
        "contrast-color",
        "device-cmyk",
        "currentcolor",
        "system-color"
    ]
}
